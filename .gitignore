n = int(input())
if n % 10 == 1 and not (n % 100) // 10 == 1 and not n == 11:
    print(n, "программист")
elif n % 10 == 2 and not (n % 100) // 10 == 1 or n % 10 == 3 and not (n % 100) // 10 == 1 or n % 10 == 4 and not (n % 100) // 10 == 1:
    print(n, 'программиста')
else:
    print(n, 'программистов')
